{
  "name": "Gmail SignOut Hotkey",
  "short_name": "Gmail Signout",
  "description": "Keyboard shortcut to quickly sign out of Gmail",
  "version": "1.0.0",
  "permissions": [
    "activeTab"
  ],

    "content_scripts": [
    {
       "matches": [ "http://*/*", "https://*/*"],
      "js": ["jquery.js", "background.js"]
    }
  ],

  "browser_action": {
    "default_icon": "cat_icon.png"
  },

  "manifest_version": 2
}


